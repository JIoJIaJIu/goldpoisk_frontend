block('g-filter-list')(
    content()(function () {
        return this.ctx.params.map(function (param, i) {
            return {
                block: 'g-filter-param',
                mods: param.values instanceof Array ? i == 2 || i == 1 || i == 5 ? { 'list': true, 'state': 'open' } : { 'list': true, 'state': 'closed' } : null,
                title: param.name,
                more: param.more,
                content: param.values
            }
        })
    })
)