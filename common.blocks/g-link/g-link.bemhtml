block('g-link')(
    tag()('a'),
    attrs()(function () {
        return {
            'href': this.ctx.url
        }
    }),
    content()(function () {
        return this.ctx.content
    }),
    mod('more', true)(
        content()(function () {
            var str = this.ctx.content || '';
            return this.ctx.content + ' &#8594;'
        })
    )
)